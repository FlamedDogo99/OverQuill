{
  "action": {},
  "background": {
    "service_worker": "background.js",
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "content-script.js"
      ],
      "css": [
        "injected.css",
        "deps/mathquill/mathquill.css"
      ],
      "matches": [
        "https://*.overleaf.com/project/*"
      ]
    }
  ],
  "description": "Interactive LaTeX editing in Overleaf, using a custom build of MathQuill",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "manifest_version": 3,
  "name": "OverQuill",
  "options_page": "options.html",
  "permissions": [
    "storage"
  ],
  "version": "1.0.3",
  "web_accessible_resources": [
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "injected.js",
        "injected.css",
        "deps/*"
      ]
    }
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "flameddogo99@gmail.com",
      "data_collection_permissions": {
        "required": ["none"]
      }
    }
  }
}