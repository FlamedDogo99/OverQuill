{
   "action": {},
   "background": {
      "service_worker": "background.js"
   },
   "content_scripts": [
     {
       "js": [ "content-script.js"],
       "css": ["injected.css", "deps/mathquill/mathquill.css"],
       "matches": [ "https://*.overleaf.com/project/*" ]
     }
   ],
   "description": "Custom keyboard shortcuts for Overleaf.",
   "icons": {
      "128": "icon128.png",
      "16": "icon16.png",
      "48": "icon48.png"
   },
   "manifest_version": 3,
   "name": "OverQuill",
   "options_page": "options.html",
   "permissions": ["storage"],
   "version": "1.0.2",
   "web_accessible_resources": [ {
      "matches": [ "<all_urls>" ],
      "resources": [ "injected.js", "injected.css", "deps/*"]
   } ]
}
